{
	"root": true,
	"extends": [
		"wesbos",
		"next"
	],
	"rules": {
		"no-console": 0,
		"react/prop-types": 0,
		"react/no-danger": 0,
		"react/destructuring-assignment": 0,
		"react/jsx-props-no-spreading": 0,
		"react/state-in-constructor": 0,
		"no-restricted-properties": 0,
		"react/display-name": 0,
		"jsx-a11y/label-has-associated-control": 0,
		"no-shadow": 0,
		"prettier/prettier": [
			"error",
			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"tabWidth": 4,
				"useTabs": true,
				"bracketSpacing": true,
				"jsxBracketSameLine": false,
				"endOfLine": "auto"
			}
		]
	}
}