{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/components/*": [
                "components/*"
            ],
            "$svg/*": [
                "components/svg/*"
            ],
            "@/utils/*": [
                "utils/*"
            ],
            "$queries/*": [
                "utils/queries/*"
            ],
            "@/styles/*": [
                "styles/*"
            ],
            "@/hooks/*": [
                "utils/queries/*"
            ]
        }
    }
}